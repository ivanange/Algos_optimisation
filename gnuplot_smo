#set term postscript color noenhanced
set term pdf color noenhanced
set output output_file
set grid
#set format ''

# color definitions
set style line 5 lc rgb 'blue'     lw 2 dt 1 pt 5
set style line 3 lc rgb 'purple'     lw 2 dt 3 pt 4
set style line 4 lc rgb 'red'      lw 2 dt 5 pt 3
set style line 2 lc rgb 'orange'   lw 2 dt 7 pt 2
set style line 1 lc rgb 'green'    lw 2 dt 9 pt 1

set title "Comparaisons";
set xlabel 'Dataset size (# samples)'
set ylabel 'Time (ms)'

set output "plots/time_comparison_SMO.pdf" 
set title "Time comparison - SMO";
set ylabel 'Time (ms)'
set key right top 
plot input_file using 1:($2) t '1 core' w lines linestyle 1, '' using 1:($3) t '2 cores' w lines linestyle 2, '' \
using 1:($4) t '4 cores' w lines linestyle 3, '' \
using 1:($5) t '6 cores' w lines linestyle 4, '' \
using 1:($6) t '8 cores' w lines linestyle 5

# without sequential
set output "plots/parallel_time_comparison_SMO.pdf" 
set title "Time comparison - SMO";
set ylabel 'Time (ms)'
set key right top 
plot input_file using 1:($3) t '2 cores' w lines linestyle 2, '' \
using 1:($4) t '4 cores' w lines linestyle 3, '' \
using 1:($5) t '6 cores' w lines linestyle 4, '' \
using 1:($6) t '8 cores' w lines linestyle 5

# without sequential and 2 cores
set output "plots/parallel_4_cores_plus_time_comparison_SMO.pdf" 
set title "Time comparison - SMO";
set ylabel 'Time (ms)'
set key right top 
plot input_file using 1:($4) t '4 cores' w lines linestyle 3, '' \
using 1:($5) t '6 cores' w lines linestyle 4, '' \
using 1:($6) t '8 cores' w lines linestyle 5